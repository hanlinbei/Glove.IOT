                function showTime(val) {
                    if (val < 60) {
                        return val + "\t秒";
                    } else {

                        var min_total = Math.floor(val / 60);	// 分钟
                        var sec = Math.floor(val % 60);	// 余秒

                        if (min_total < 60) {
                            return min_total + "\t分钟" + sec + "\t秒";
                        } else {
                            var hour_total = Math.floor(min_total / 60);	// 小时数
                            var min = Math.floor(min_total % 60);	// 余分钟

